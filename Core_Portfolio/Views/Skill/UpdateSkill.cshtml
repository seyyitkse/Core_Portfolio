@model EntityLayer.Concrete.Skill
@{
    ViewData["Title"] = "UpdateSkill";
    Layout = "~/Views/Shared/_NewAdminLayout.cshtml";
}
<div class="card-header border-0">
    <h3 class="mb-0">Update Skill</h3>
</div>
<form class="form-control" method="post">
    <br />
    <br />
    <br />
    <br />
    @Html.HiddenFor(x=>x.SkillID)
    @Html.Label("Skill Title:")
    -    <br />
    @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
    <br />
    <br />
    @Html.Label("Skill Value:")
    <br />
    @Html.TextBoxFor(x => x.Value, new { @class = "form-control" })
    <br />
    <button class="btn btn-primary" id="btnAdd"> Add Skill</button>
</form>


